{
	"name": "jordioliu/todevise20",
	"description": "Todevise 2.0",
	"keywords": ["todevise", "store"],
	"homepage": "http://www.todevise.com/",
	"type": "project",
	"license": "Proprietary",
	"support": {
		"issues": "https://app.asana.com/0/24428065353423/24428065353423",
		"source": "https://github.com/jordioliu/todevise20"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.5.0",
		"yiisoft/yii2": "2.0.3",
		"yiisoft/yii2-bootstrap": "2.0.3",
		"yiisoft/yii2-swiftmailer": "2.0.3",
		"yiisoft/yii2-mongodb": "2.0.3",

		"lajax/yii2-language-picker": "1.0.1",
		"alexandernst/yii2-device-detect": "0.0.5",

		"npm-asset/angular": "1.3.15"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "2.0.3",
		"yiisoft/yii2-debug": "2.0.3",
		"yiisoft/yii2-gii": "2.0.3",
		"yiisoft/yii2-faker": "2.0.3"
	},
	"config": {
		"process-timeout": 1800
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::postCreateProject"
		]
	},
	"extra": {
		"yii\\composer\\Installer::postCreateProject": {
			"setPermission": [
				{
					"runtime": "0777",
					"web/assets": "0777",
					"yii": "0755"
				}
			],
			"generateCookieValidationKey": [
				"config/web.php"
			]
		},
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	}
}
