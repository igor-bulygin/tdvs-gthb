{
	"name": "todevise20",
	"description": "Todevise 2.0",
	"version": "2.0.0",
	"keywords": ["todevise", "store"],
	"homepage": "http://www.todevise.com/",
	"type": "project",
	"license": "Proprietary",
	"support": {
		"issues": "https://app.asana.com/0/24428065353423/24428065353423",
		"source": "https://github.com/jordioliu/todevise20"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.6.0",
		"yiisoft/yii2": "2.0.4",
		"yiisoft/yii2-bootstrap": "2.0.4",
		"yiisoft/yii2-swiftmailer": "2.0.4",
		"yiisoft/yii2-mongodb": "2.0.4",
		"yiisoft/yii2-redis": "2.0.4",

		"kartik-v/yii2-widget-sidenav": "1.0.0",
		"lajax/yii2-language-picker": "1.1.5",
		"alexandernst/yii2-device-detect": "0.0.7",
		"alexandernst/yii2-scrypt": "0.0.4"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "2.0.4",
		"yiisoft/yii2-debug": "2.0.4",
		"yiisoft/yii2-gii": "2.0.4",
		"yiisoft/yii2-faker": "2.0.3"
	},
	"config": {
		"process-timeout": 1800
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::postCreateProject"
		],
		"post-install-cmd": [
			"npm install",
			"bower install",
			"rsync -aX node_modules/ vendor/npm/",
			"rsync -aX bower_components/ vendor/bower/",
			"rm -rf node_modules bower_components"
		],
		"post-update-cmd": [
			"composer run-script post-install-cmd"
		]
	},
	"extra": {
		"yii\\composer\\Installer::postCreateProject": {
			"setPermission": [
				{
					"runtime": "0777",
					"web/assets": "0777",
					"yii": "0755"
				}
			],
			"generateCookieValidationKey": [
				"config/web.php"
			]
		},
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		}
	}
}
